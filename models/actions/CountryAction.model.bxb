action (CountryAction) {
  description (Handle request for country info)
  type (Search)
  collect {
    input (countryName) {
      type (CountryName)
      min (Required) max (One)
    }
    input (countryCode) {
      type (CountryCode)
      min (Optional) max (One)
    }
    input (information) {
      type (Information)
      min (Optional) max (Many)
    }
    input (resetInformationFlag) {
      type (ResetInformationFlag)
      min (Optional) max (One)
    }
  }
  output (Country)
}